	        LEFT OUTER JOIN CLAIM_TEMP OOC ON																				
	            OC.OD_ORDER_ID = OOC.OD_ORDER_ID																				
	          AND OOD.OD_ORDER_DETL_ID = OOC.OD_ORDER_DETL_ID																				
	        LEFT OUTER JOIN PS_CLAIM_BOS PCB ON																				
	            OCD.PS_CLAIM_BOS_ID = PCB.PS_CLAIM_BOS_ID																				
