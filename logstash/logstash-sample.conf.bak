	        SELECT																				
	          OOD.OD_ORDER_ID																				
	            ,																				
	          OS.STATUS_CD																				
	            ,																				
	          MIN(OS.ORDER_STATUS_SORT) AS ORDER_STATUS_SORT																				
	        FROM																				
	          OD_STATUS OS																				
	            INNER JOIN OD_ORDER_DETL OOD ON																				
	              OOD.ORDER_STATUS_CD = OS.STATUS_CD																				
