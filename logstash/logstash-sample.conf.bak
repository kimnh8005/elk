	        LEFT OUTER JOIN PS_CLAIM_CTGRY PCC3 ON																				
	          PCB.S_CLAIM_CTGRY_ID = PCC3.PS_CLAIM_CTGRY_ID																				
	        /* 클레임 테이블 END */																				
